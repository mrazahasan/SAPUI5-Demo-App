<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<ColumnListItem
		type="Navigation"
        press="onListItemPress">
		<cells>
			<ObjectIdentifier
				title="{Name}"
				text="{ProductId}" />
			<Text
				text="{noticedate}" />                
			<Text
				text="{status}" />		
            <Text
                text="{expirydate}" />
            <Text
				text="{signeddate}" />
		</cells>
	</ColumnListItem>
</core:FragmentDefinition>